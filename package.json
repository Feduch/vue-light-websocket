{
  "name": "vue-light-websocket",
  "version": "0.1.4",
  "description": "A simple native reconnecting websocket for Vue.js",
  "main": "dist/VueLightWebSocket.js",
  "module": "dist/VueLightWebSocket.module.js",
  "homepage": "https://github.com/Feduch/vue-light-websocket",
  "repository": "github:Feduch/vue-light-websocket",
  "scripts": {
    "clean": "rimraf dist",
